{
  "name": "@rfgamaral/parcel-plugin-typings-for-css-modules",
  "description": "A plugin for the Parcel bundler to generate .d.ts files for your CSS Modules.",
  "version": "1.0.2",
  "main": "./lib/index.js",
  "author": {
    "name": "Ricardo Amaral",
    "email": "master@ricardoamaral.net"
  },
  "license": "MIT",
  "repository": "https://github.com/rfgamaral/parcel-plugin-typings-for-css-modules",
  "bugs": "https://github.com/rfgamaral/parcel-plugin-typings-for-css-modules/issues",
  "keywords": [
    "css",
    "css-modules",
    "modules",
    "parcel",
    "parcel-bundler",
    "type-definitions",
    "typed-css-modules",
    "types",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "release": "semantic-release"
  },
  "dependencies": {
    "cosmiconfig": "5.2.1",
    "dot-prop": "5.1.1",
    "typed-css-modules": "0.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@semantic-release/changelog": "3.0.5",
    "@semantic-release/git": "7.0.16",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "husky": "3.0.9",
    "parcel-bundler": "1.12.4",
    "prettier": "1.18.2",
    "semantic-release": "15.13.28"
  },
  "peerDependencies": {
    "parcel-bundler": "^1.11.0"
  }
}
